<div> 
    <div class="page-breadcrumb" >
        <div class="row">
            <div class="col-5 align-self-center">
                <h4 class="page-title">Form Horizontal</h4>
                <div class="d-flex align-items-center">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Library</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>

    <div class="container-fluid">

        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title" style="text-align: center;">VENDOR PERSONAL SETTINGS</h4>
                        <h6 class="card-subtitle"></h6>
                    </div>
                    <hr>

                    <div class="card-body">
                        <h4 class="card-title">VENDOR INFO</h4>
                        <div class="form-group row">
                            <label for="fname" class="col-sm-3 text-right control-label col-form-label">Vendor Id</label>
                            <div class="col-sm-9">
                                <p class= "form-control">
                                    <%= @vendor.id %>
                                </p>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname" class="col-sm-3 text-right control-label col-form-label">First Name</label>
                            <div class="col-sm-9">
                                <p class= "form-control">
                                    <%= @vendor.first_name %>
                                </p>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname" class="col-sm-3 text-right control-label col-form-label">First Name</label>
                            <div class="col-sm-9">
                                <p class= "form-control">
                                    <%= @vendor.last_name %>
                                </p>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="email1" class="col-sm-3 text-right control-label col-form-label">Email</label>
                            <div class="col-sm-9">
                                <p class= "form-control">
                                    <%= @vendor.email %>
                                </p>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>

</div>



