
<div class="main-wrapper">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Login box.scss -->
    <!-- ============================================================== -->
    <div class="auth-wrapper d-flex no-block justify-content-center align-items-center" 
         style="background:url(/assets/auth-bg.jpg) no-repeat center center;    padding-top: 10%;
         padding-bottom: 10%;">
        <div class="auth-box" style="
             border: 1px solid #ddd;
             width: 450px;
             background: #fff;
             ">
            <div id="loginform" style="
                 padding: 30px;">
                <div class="logo" style="    text-align: center;
                     padding-bottom: 10px;
                     padding-top: 10px;">
                    <span class="db">
                        <img src="/assets/logo-icon.png" alt="logo" />
                    </span>
                    <h5 class="font-medium m-b-20">Sign In to Admin</h5>
                </div>
                
                    <div class="col-12">
                    <%= form_for :user, url: users_url do |f|  %>
                        
                          <form class="form-horizontal m-t-20">
                              
                              <div class="form-group row">
                                  <div class="col-12 ">
                                      <%= f.text_field :first_name, autofocus: true ,:placeholder=>"John",:class => "form-control form-control-lg", required: true %>

                                  </div>
                              </div>
                              <div class="form-group row ">
                                  <div class="col-12 ">
                                      <%= f.text_field :last_name, autofocus: true ,:placeholder=>"Doe",:class => "form-control form-control-lg" ,required: true%>
                                  </div>
                              </div>
                              
                              <div class="form-group row">
                                  <div class="col-12 ">
                                      <%= f.email_field :email, autofocus: true,:placeholder=>"johndoe@domain.com",:class => "form-control form-control-lg" ,required: true %>
                                  </div>
                              </div>
                              <div class="form-group row">
                                  <div class="col-12 ">
                                      <%= f.password_field :password, autocomplete: "off" ,:placeholder=>"Password",:class => "form-control form-control-lg" ,required: true%>
                                  </div>
                              </div>
                              <div class="form-group row">
                                  <div class="col-12 ">
                                      <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder=>"Password Confirmation",:class => "form-control form-control-lg" ,required: true%>
                                  </div>
                              </div>
                             
                             


                              
                              <div class="form-group text-center ">
                                  <div class="col-xs-12 p-b-20 ">
                                      <button class="btn btn-block btn-lg btn-info " type="submit ">SIGN UP</button>
                                  </div>
                              </div>
                              <div class="form-group m-b-0 m-t-10 ">
                                  <div class="col-sm-12 text-center ">
                                      Already have an account? <a href="<%= signin_users_url %>" rel="login" class="text-info m-l-5 "><b>Sign In</b></a>
                                  </div>
                              </div>
                          </form>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Login box.scss -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
