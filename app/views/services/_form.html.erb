<% if @already.blank? %>
<%= form_for(@pool_service, url: services_url) do |f| %>
<table id="demo-foo-addrow" class="table m-t-30 no-wrap table-hover contact-list" data-page-size="10">
    <% if current_page?(spa_service_services_url) %>
    <%= f.hidden_field :service_type,:placeholder=>"7.3",:class=>"form-control",:value => "spa"%>
    <% else %>
    <%= f.hidden_field :service_type,:placeholder=>"7.3",:class=>"form-control",:value => "pool"%>
    <% end %>
      <thead>
          <tr>
              <th>Pool Chemical Ranges</th>
              <th style="text-align: center;">Low Range</th>
              <th style="text-align: center;">High Range</th>
              <th>Select</th>
          </tr>
      </thead>
      <tbody>

          <tr>
              <td>PH:</td>
              <td><%= f.text_field :ph_low,:placeholder=>"7.3",:class=>"form-control"%></td>
              <td><%= f.text_field :ph_high,:placeholder=>"7.6",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :ph_select %></td>
          </tr>
          <tr>
              <td>CH:</td>
              <td><%= f.text_field :ch_low,:placeholder=>"3 ppm",:class=>"form-control"%></td>
              <td><%= f.text_field :ch_high,:placeholder=>"7 ppm",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :ch_select %></td>
          </tr>
          <tr>
              <td>Combined CH:</td>
              <td><%= f.text_field :combined_ch_low,:placeholder=>"0 ppm",:class=>"form-control"%></td>
              <td><%= f.text_field :combined_ch_high,:placeholder=>"1 ppm",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :combined_select %></td>
          </tr>
          <tr>
              <td>Alkalinity:</td>
              <td><%= f.text_field :alkalinity_low,:placeholder=>"90",:class=>"form-control"%></td>
              <td><%= f.text_field :alkalinity_high,:placeholder=>"150",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :alkalinity_select %></td>
          </tr>
          <tr>
              <td>Calcium Hardness:</td>
              <td><%= f.text_field :calcium_hardness_low,:placeholder=>"200",:class=>"form-control"%></td>
              <td><%= f.text_field :calcium_hardness_high,:placeholder=>"400",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :calcium_select %></td>
          </tr>
          <tr>
              <td>Stabilizer:</td>
              <td><%= f.text_field :stabilizer_low,:placeholder=>"30 ppm",:class=>"form-control"%></td>
              <td><%= f.text_field :stabilizer_high,:placeholder=>"60 ppm",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :stabilizer_select %></td>
          </tr>
          <tr>
              <td>Salt:</td>
              <td><%= f.text_field :salt_low,:placeholder=>"2300 ppm",:class=>"form-control"%></td>
              <td><%= f.text_field :salt_high,:placeholder=>"3400 ppm",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :salt_select %></td>
          </tr>
          <tr>
              <td>Total Dissolved Soiled:</td>
              <td><%= f.text_field :dissolved_soild_low,:placeholder=>"0 ppm",:class=>"form-control"%></td>
              <td><%= f.text_field :dissolved_soild_high,:placeholder=>"2000 ppm",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :dissolved_select %></td>
          </tr>
          <tr>
              <td>Saturation Index:</td>
              <td><%= f.text_field :saturation_index_low,:placeholder=>"-0.4",:class=>"form-control"%></td>
              <td><%= f.text_field :saturation_index_high,:placeholder=>"0.4",:class=>"form-control"%></td>
              <td align="center"><%= f.check_box :saturation_select %></td>
          </tr>   
      </tbody>
      <tfoot>
          <tr>
              <td colspan="2">
                  <%= f.hidden_field :user_id,:value=>current_user.id%>
                  <%= hidden_field_tag "service_type",nil,:value=>params[:action]%>
              </td>

              <td colspan="7" align="right">
                  <%=f.submit "save",:class=>"btn btn-info btn-rounded",style: "margin-right: 3%;"%>

              </td>
          </tr>
      </tfoot>

  </table>
<% end %>

